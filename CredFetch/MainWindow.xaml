<Window Name="WindowMain"
        x:Class="CredFetch.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="CredFetch"
        Width="210"
        Height="180"
        ResizeMode="CanMinimize">
    <Grid Name="GridLayout">
        <PasswordBox Name="KeyBox" Password="" HorizontalAlignment="Center" Margin="0,15,0,0" VerticalAlignment="Top"
                     Height="25" Width="150" FontSize="17" />
        <PasswordBox Name="IvBox" Password="" HorizontalAlignment="Center" Margin="0,55,0,0" VerticalAlignment="Top"
                     Height="25"
                     Width="150" FontSize="17" />
        <Button Name="ButtonOk" Click="DecryptConfigFileBtn" Content="OK" HorizontalAlignment="Center"
                Margin="0,95,0,0" VerticalAlignment="Top" Width="150" Height="25" FontSize="15" />
    </Grid>
</Window>